# Headless Trader Configuration
# Copy this to .env and fill in your values

# Alpaca API Credentials
ALPACA_API_KEY=your_api_key_here
ALPACA_SECRET_KEY=your_secret_key_here

# Trading Mode (true = paper trading, false = live trading)
ALPACA_PAPER=true

# Enable/Disable Trading (set to "true" to execute real trades)
# When false, signals will be logged but NO trades will be executed
TRADING_ENABLED=false

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
